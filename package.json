{
  "name": "wit-commander",
  "version": "0.0.1",
  "description": "WordPress plugin that provides base functionality for use with Wit Digital theme and plugins.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/wit-digital/wit-plugins/wit-commander.git"
  },
  "keywords": [],
  "author": "Ryan E. Mitchell",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/wit-digital/wit-plugins/wit-commander/issues"
  },
  "homepage": "https://gitlab.com/wit-digital/wit-plugins/wit-commander#readme",
  "devDependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@babel/preset-react": "^7.9.4",
    "@tinypixelco/laravel-mix-wp-blocks": "^1.0.0",
    "@wordpress/babel-preset-default": "^4.13.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.6.0",
    "apollo-boost": "^0.4.9",
    "cross-env": "^7.0.2",
    "graphql": "^15.0.0",
    "laravel-mix": "^5.0.4",
    "laravel-mix-copy-watched": "^2.2.4",
    "laravel-mix-purgecss": "^5.0.0",
    "laravel-mix-wp-blocks": "^0.2.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "websocket-extensions": ">=0.1.4"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "aos": "^2.3.4",
    "classnames": "^2.2.6",
    "lodash.assign": "^4.2.0"
  }
}
